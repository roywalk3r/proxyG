NODE_ENV=production
PORT=3001

# Cache Configuration
MAX_CACHE_SIZE=500
CACHE_TTL=300000
PLAYLIST_CACHE_TTL=30000

# Request Configuration
MAX_RETRIES=3
TIMEOUT=30000
MAX_CONCURRENT_REQUESTS=100
# Default outbound headers
DEFAULT_REFERER=https://tmstr2.shadowlandschronicles.com
DEFAULT_ORIGIN=https://tmstr2.shadowlandschronicles.com

# Rate Limiting
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=2000
STRICT_RATE_LIMIT_MAX=500

# Security Configuration
ALLOWED_ORIGINS=*
# ALLOWED_ORIGINS=https://yourdomain.com,https://www.yourdomain.com
BLOCKED_DOMAINS=localhost,127.0.0.1,0.0.0.0,::1
# ALLOWED_DOMAINS=example.com,cdn.example.com,*.streamingprovider.com

# Health Check
HEALTH_CHECK_INTERVAL=30000

